.q-card{
    background: rgba(255,255,255,0.9);
    min-height: 183px;
      -webkit-animation-name: fade-cards;
  /* Safari 4.0 - 8.0 */
  -webkit-animation-duration: 1s;
  /* Safari 4.0 - 8.0 */
  animation-name: fade-cards;
  animation-duration: 1s;
  transition: .2s;
}

/* Safari 4.0 - 8.0 */
@-webkit-keyframes fade-cards {
  0% {
    margin-top: 100%;
  }

  100% {
    margin-top: 0%;
  }
}

/* Standard syntax */
@keyframes fade-cards {
  0% {
    margin-top: 500px;
  }

  100% {
    margin-top: 0;
  }
}
